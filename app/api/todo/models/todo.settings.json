{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "name": "Todo"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "due": {
      "type": "date"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "minor",
        "regular",
        "critical"
      ],
      "default": "regular",
      "required": true
    },
    "done": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
